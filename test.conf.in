# Meanwhile Python Test Script Configuration File
#
# test.conf is read by the test shell script and used to provide login
# information to Python.

# User name
export mw_user=MyTestUser

# Password
export mw_pass=MyPassword

# Host to connect to.
export mw_host=server.domain.net

# Port to connect to. Default is normally 1533
export mw_port=1533

# Which python binary to run the test in, and optionally where to look
# for python libs
export py_bin=`which python`
export py_lib=

